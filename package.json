{
  "name": "mobility-platform",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "dev:api": "pnpm --filter api dev",
    "dev:web": "pnpm --filter web dev",
    "build": "pnpm run -r build",
    "verify:urls": "node scripts/verify-urls.mjs",
    "verify:listings": "node scripts/verify-listings-pages.mjs",
    "db:generate": "pnpm --filter database generate",
    "db:migrate": "dotenv -e .env -- pnpm --filter database migrate:deploy",
    "db:seed": "dotenv -e .env -- pnpm --filter database db:seed",
    "db:studio": "dotenv -e .env -- pnpm --filter database studio",
    "db:test:reset": "node scripts/db-test-reset.mjs",
    "kill:ports": "node scripts/kill-ports.mjs",
    "test:e2e": "pnpm --filter web test:e2e:install-browsers && pnpm --filter web test:e2e:no-server"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.0.0"
}
